   268                        ; Fetch the next character from the RX buffer waiting for some to arrive if the
   269                        ; buffer is empty.
   270                        
   271                                        public  UartRx
   272                        UartRx:
   273 00:00D1: 08                           php                             ; Save current MX settings
   274                                        short_a                         ; .. and ensure 8-bits
+  274 00:00D2: E2 20                        sep     #(1<<M_FLAG)
+  274                                        longa   off
   275                        RxWait:
   276 00:00D4: AD 81 7F                     lda     ACIA_SR                 ; Any data in the RX buffer
   277 00:00D7: 29 08                        and     #$08
   278 00:00D9: F0 F9                        beq     RxWait                  ; No, wait for some
   279 00:00DB: AD 80 7F                     lda     ACIA_RXD                ; Recover the received data
   280 00:00DE: 28                           plp
   281 00:00DF: 60                           rts                             ; Done
